{
  "name": "bikedepot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/apps/shop/server.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/apps/shop/server.ts",
    "build": "tsc -p tsconfig.json",
    "migrate": "ts-node -r tsconfig-paths/register src/apps/shop/command/dynamodb.migrate.ts",
    "test": "jest --no-cache",
    "test:watch": "jest --watch --no-cache",
    "lint": "eslint . --fix",
    "format": "prettier --write src/ tests/",
    "deploy": "serverless deploy",
    "offline": "serverless offline"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.772.0",
    "@aws-sdk/lib-dynamodb": "^3.772.0",
    "@hono/node-server": "^1.13.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "hono": "^4.7.4",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@faker-js/faker": "^9.6.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.22.0",
    "jest": "^29.7.0",
    "jiti": "^2.4.2",
    "nodemon": "^3.1.9",
    "prettier": "3.5.3",
    "serverless": "^4.9.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^14.4.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  }
}
